{
    "env": {
        "browser": true,
        "es2020": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 11
    },
    "rules": {
        "linebreak-style": ["error", "windows"],
        "no-use-before-define": "off",
        "import/prefer-default-export": "off",
        "no-console": "off",
        "import/extensions": "off"
    }
}
